<table style="margin: 0 auto; width: 100%; max-width: 400px; background-color: lightgray; border-collapse: separate; border-spacing: 5px;">
    <thead>
        <tr>
            <th style="background-color: green; color: white; padding: 6px; text-align: center; font-size: 14px;">No</th>
            <th style="background-color: green; color: white; padding: 6px; text-align: center; font-size: 14px;">Plate No</th>
            <th style="background-color: green; color: white; padding: 6px; text-align: center; font-size: 14px;">Side No</th>
            <th style="background-color: green; color: white; padding: 6px; text-align: center; font-size: 14px;">No Seats</th>
            <th style="background-color: green; color: white; padding: 6px; text-align: center; font-size: 14px;">Action</th>
        </tr>
    </thead>
    <tbody style="background-color: lightgray;">
        {% for bus in buses %}
        <tr>
            <td style="text-align: center; color: brown; padding: 8px; margin-right: 10px;">
                <h4 style="margin: 0;">{{ forloop.counter }}</h4>
            </td>
            <td>
                <form action="#" method="POST">
                    {% csrf_token %}
                    <input type="text" name="plate_no" value="{{ bus.plate_no }}" readonly
                        style="height: 35px; width: 70px; text-align: center; color: blue; border-radius: 5px; border: 1px solid #ccc; font-weight: bold; margin: 5px; padding: 5px;">
            </td>
            <td>
                <input type="text" name="sideno" value="{{ bus.sideno }}" readonly
                    style="height: 35px; width: 70px; text-align: center; color: blue; border-radius: 5px; border: 1px solid #ccc; font-weight: bold; margin: 5px; padding: 5px;">
            </td>
            <td>
                <input type="text" name="no_seats" value="{{ bus.no_seats }}" readonly
                    style="height: 35px; width:70px; text-align: center; color: blue; border-radius: 5px; border: 1px solid #ccc; font-weight: bold; margin: 5px; margin-top:5px;padding: 5px;">
            </td>
            <td style="text-align: center;">
		    <!--
                <button type="submit" class="btn btn-danger" style="width: 70px; height: 30px; text-align:center; margin-right:10px;
		margin-buttom:10px;">Delete</button>-->
	<button type="submit" class="btn btn-danger"
        style="width: 70px; height: 30px; display: flex; align-items: center; justify-content: center; margin-right: 10px; margin-bottom: 10px;">
    Delete
</button>
            </td>
        </tr>
        {% endfor %}
    </tbody>
</table>
