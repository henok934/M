<table style="margin: 20px auto; width: 100%; max-width: 600px; background-color: lightgray; border-collapse: collapse;">
    <thead>
        <style>
            th {
                background-color: #4CAF50; /* Green background */
                color: white;              /* White text */
                padding: 10px;            /* Padding for cells */
                text-align: center;        /* Align text to the center */
            }
            tr:nth-child(even) {
                background-color: #f2f2f2; /* Zebra striping for rows */
            }
            td {
                padding: 10px;             /* Padding for cells */
            }
            input {
                height: 30px;              /* Reduced height */
                width: calc(100% - 10px);  /* Full width minus padding */
                margin: 0;                 /* Reset margin */
                padding: 5px;              /* Padding inside input */
                text-align: center;        /* Center text */
                color: blue;               /* Text color */
                border-radius: 5px;       /* Rounded corners */
                border: 1px solid #ccc;    /* Border style */
                font-weight: bold;         /* Bold text */
                box-sizing: border-box;    /* Include padding and border in width */
                font-size: 14px;           /* Font size */
            }

            /* Responsive adjustments */
            @media (max-width: 600px) {
                table {
                    width: 95%; /* Use more width on smaller screens */
                    margin: 10px; /* Margin for small screens */
                }
                input {
                    width: 100%; /* Full width for inputs */
                }
            }
        </style>
        <tr>
            <th style="width: 50px;">No</th>
            <th style="width: 120px;">Travel Date</th>
            <th style="width: 100px;">Departure</th>
            <th style="width: 100px;">Destination</th>
            <th style="width: 100px;">Plate No</th>
            <th style="width: 100px;">Side No</th>
            <th style="width: 80px;">Action</th>
        </tr>
    </thead>
    <tbody>
        {% for route in routes %}
        <tr>
            <td style="text-align: center; color: brown;">
                <h3>{{ forloop.counter }}</h3>
            </td>
            <td>
                <input type="text" name="date" value="{{ route.date|slice:10 }}" readonly>
            </td>
            <td>
                <input type="text" name="depcity" value="{{ route.depcity|slice:10 }}" readonly>
            </td>
            <td>
                <input type="text" name="descity" value="{{ route.descity|slice:10 }}" readonly>
            </td>
            <td>
                <input type="text" name="plate_no" value="{{ route.plate_no|slice:10 }}" readonly>
            </td>
            <td>
                <input type="text" name="side_no" value="{{ route.side_no|slice:10 }}" readonly>
            </td>
            <td>
                <form action="{% url 'routedelete' %}" method="POST">
                    {% csrf_token %}
                    <button type="submit" class="btn btn-danger" style="width: 80px;">
                        Delete
                    </button>
                </form>
            </td>
        </tr>
        {% endfor %}
    </tbody>
</table>

